{
  "extends": "tslint:recommended",
  "rules": {
      "comment-format": false,
      "interface-name": false,
      "jsdoc-format": false,
      "max-line-length": false,
      "member-access": false,
      "member-ordering": false,
      "no-empty": false,
      "no-namespace": false,
      "no-shadowed-variable": false,
      "quotemark": false
  }
}
